</span><span><span class="hljs-keyword">from</span></span><span> textblob </span><span><span class="hljs-keyword">import</span></span><span> TextBlob

</span><span><span class="hljs-comment"># Load Facebook data (assuming JSON format)</span></span><span>
</span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(</span><span><span class="hljs-string">"facebook_data.json"</span></span><span>, </span><span><span class="hljs-string">"r"</span></span><span>, encoding=</span><span><span class="hljs-string">"utf-8"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
    data = json.load(f)

</span><span><span class="hljs-comment"># Extract all text (e.g., posts + comments)</span></span><span>
all_text = </span><span><span class="hljs-string">" "</span></span><span>.join([post.get(</span><span><span class="hljs-string">"text"</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>) </span><span><span class="hljs-keyword">for</span></span><span> post </span><span><span class="hljs-keyword">in</span></span><span> data[</span><span><span class="hljs-string">"posts"</span></span><span>]])

</span><span><span class="hljs-comment"># Perform sentiment analysis</span></span><span>
blob = TextBlob(all_text)
sentiment = blob.sentiment.polarity

</span><span><span class="hljs-keyword">if</span></span><span> sentiment &gt; </span><span><span class="hljs-number">0</span></span><span>:
    sentiment_result = </span><span><span class="hljs-string">"Positive"</span></span><span>
</span><span><span class="hljs-keyword">elif</span></span><span> sentiment &lt; </span><span><span class="hljs-number">0</span></span><span>:
    sentiment_result = </span><span><span class="hljs-string">"Negative"</span></span><span>
</span><span><span class="hljs-keyword">else</span></span><span>:
    sentiment_result = </span><span><span class="hljs-string">"Neutral"</span></span><span>

</span><span><span class="hljs-comment"># Count occurrences of your name "Ramya"</span></span><span>
your_name = </span><span><span class="hljs-string">"Ramya"</span></span><span>
name_count = all_text.lower().count(your_name.lower())

</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"Sentiment of Facebook data:"</span></span><span>, sentiment_result)
</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"Occurrences of '<span class="hljs-subst">{your_name}</span></span></span><span>':", name_count)
</span></span></code></div></div></pre>
